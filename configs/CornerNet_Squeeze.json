{
    "system": {
        "dataset": "COCO",
        "batch_size": 48,
        "sampling_function": "cornernet",

        "train_split": "trainval",
        "val_split": "minival",
        "test_split": "testdev",

        "learning_rate": 0.001,
        "decay_rate": 10,

        "val_iter": 0,

        "opt_algo": "adam",
        "prefetch_size": 8,

        "max_iter": 200000,
        "stepsize": 100000,
        "snapshot": 5000,

        "chunk_sizes": [12,12,12,12],

        "data_dir": "./data"
    },
    
    "db": {
        "rand_scale_min": 0.6,
        "rand_scale_max": 1.4,
        "rand_scale_step": 0.1,
        "rand_scales": null,

        "rand_crop": true,
        "rand_color": true,

        "border": 128,
        "gaussian_bump": true,
        "gaussian_iou": 0.3,

        "input_size": [511, 511],
        "output_sizes": [[64, 64]],

        "test_scales": [1],
        "test_flipped": false,

        "top_k": 10,
        "num_dets": 20,
        "categories": 20,
        "ae_threshold": 0.5,
        "nms_threshold": 0.5,

        "max_per_image": 20
        
    }
}
