{
    "system": {
        "dataset": "COCO",
        "batch_size": 6,
        "sampling_function": "cornernet",

        "train_split": "trainval",
        "val_split": "minival",
        "test_split": "testdev",

        "learning_rate": 0.005,
        "decay_rate": 5,

        "val_iter": 100,

        "opt_algo": "adam",
        "prefetch_size": 3,

        "max_iter": 100000,
        "stepsize": 10000,
        "snapshot": 5000,

        "chunk_sizes": [3, 3],

        "data_dir": "./data"
    },
    
    "db": {
        "rand_scale_min": 0.7,
        "rand_scale_max": 1.2,
        "rand_scale_step": 0.1,
        "rand_scales": null,

        "rand_crop": true,
        "rand_color": true,

        "border": 128,
        "gaussian_bump": true,
        "gaussian_iou": 0.3,

        "input_size": [511, 511],
        "output_sizes": [[128, 128]],

        "test_scales": [1],

        "top_k": 100,
        "categories": 15,
        "ae_threshold": 0.5,
        "nms_threshold": 0.5,

        "max_per_image": 100
    }
}
